    "morgan": "^1.9.1",
    "mysql": "^2.15.0",
    "pg": "^6.1.0",
    "pug": "^2.0.0-rc.4",
    "raven": "^2.4.2",
    "sequelize": ">=4.44.3",
    "sequelize-cli": "^3.1.0",
    "serve-favicon": "~2.3.0",
    "sqlite3": "^3.0.8"
  },
  "devDependencies": {
    "cross-env": "^3.1.3",
